{
    "train_params": {
        "epochs": 200,
        "callbacks": [
            {
                "class_name": "EarlyStopping",
                "config": {
                    "monitor": "val_loss",
                    "patience": 30
                }
            }
        ]
    },
    "dataset_params": {
        "class_name": "H5Reader",
        "config": {
            "filename": "/mnt/users/ngochuyn/datasets/headneck/outcome.h5",
            "batch_size": 4,
            "batch_cache": 7,
            "x_name": "image",
            "y_name": "DFS",
            "train_folds": [
                1,
                2,
                3,
                4
            ],
            "val_folds": [
                0
            ],
            "test_folds": [
                0
            ],
            "shuffle": true,
            "preprocessors": [
                {
                    "class_name": "HounsfieldWindowingPreprocessor",
                    "config": {
                        "window_center": 70,
                        "window_width": 200,
                        "channel": 0
