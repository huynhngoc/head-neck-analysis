        },
        "metrics": [
            {
                "class_name": "BinaryFbeta"
            },
            {
                "class_name": "AUC"
            },
            {
                "class_name": "BinaryCrossentropy"
            },
            {
                "class_name": "TruePositives",
                "config": {
                    "name": "TP"
                }
            },
            {
                "class_name": "FalsePositives",
                "config": {
                    "name": "FP"
                }
            },
            {
                "class_name": "TrueNegatives",
                "config": {
                    "name": "TN"
                }
            },
            {
                "class_name": "FalseNegatives",
                "config": {
                    "name": "FN"
                }
            }
        ]
    },
    "architecture": {
        "type": "MultiInputModelLoader",
        "layers": [
            {
                "class_name": "Conv3D",
