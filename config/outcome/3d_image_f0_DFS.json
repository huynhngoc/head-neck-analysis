            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "GlobalAveragePooling3D"
            },
            {
                "class_name": "Dense",
                "config": {
                    "units": 1024,
                    "activation": "relu"
                }
            },
            {
                "name": "image_path",
                "class_name": "Dense",
                "config": {
                    "units": 256,
                    "activation": "relu"
                }
            },
            {
                "class_name": "Dense",
                "config": {
                    "units": 1,
                    "activation": "sigmoid"
                }
            }
        ]
    }
