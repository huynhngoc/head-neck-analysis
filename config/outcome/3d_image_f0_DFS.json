                        "vmax": [
                            100,
                            25
                        ]
                    }
                }
            ],
            "augmentations": {
                "class_name": "ClassImageAugmentation3D",
                "config": {
                    "rotation_range": 90,
                    "zoom_range": [
                        0.95,
                        1.05
                    ],
                    "shift_range": [
                        10,
                        10,
                        10
                    ],
                    "flip_axis": 1,
                    "brightness_range": [
                        0.8,
                        1.2
                    ],
                    "contrast_range": [
                        0.7,
                        1.3
                    ],
                    "noise_variance": 0.05,
                    "noise_channel": 1,
                    "blur_range": [
                        0.5,
                        1.5
                    ],
                    "blur_channel": 1
                }
            }
        }
    },
    "input_params": {
        "shape": [
            null,
            null,
            null,
            3
        ]
    },
    "model_params": {
        "loss": {
            "class_name": "FusedLoss",
            "config": {
                "loss_configs": [
                    {
                        "class_name": "BinaryCrossentropy"
                    },
                    {
                        "class_name": "BinaryMacroFbetaLoss"
                    }
                ]
            }
        },
        "optimizer": {
            "class_name": "adam",
            "config": {
                "learning_rate": 0.0001
